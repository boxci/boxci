{
  "name": "boxci",
  "version": "0.0.1-beta.1",
  "license": "MIT",
  "description": "The Box CI CLI. For documentation run 'boxci' or see https://boxci.dev/docs",
  "bin": "./bin/boxci.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/boxci/boxci.git"
  },
  "scripts": {
    "typecheck": "sh ./scripts/typecheck.sh",
    "test": "sh ./test/run-tests.sh",
    "integration-test": "sh ./test/integration-test.sh",
    "package": "npx --no-install webpack --config ./webpack.config.js --mode production",
    "build": "npm run package",
    "prepublishOnly": "npm run build",
    "self-ci": "sh ./scripts/self-ci-build.sh",
    "analyse": "ANALYSE=true npx --no-install webpack --config ./webpack.config.js --mode production"
  },
  "dependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.17.0",
    "@types/js-yaml": "3.12.1",
    "@types/node-fetch": "2.5.4",
    "@types/ora": "3.2.0",
    "@types/rimraf": "2.0.3",
    "@types/shelljs": "0.8.5",
    "@types/uuid": "3.4.5",
    "@types/yaml": "1.2.0",
    "@types/yamljs": "0.2.30",
    "body-parser": "1.19.0",
    "commander": "3.0.1",
    "cookie-parser": "1.4.4",
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "js-yaml": "3.13.1",
    "node-fetch": "2.6.0",
    "ora": "3.4.0",
    "prettier": "1.18.2",
    "rimraf": "3.0.0",
    "shelljs": "0.8.3",
    "simple-git": "1.126.0",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "uuid": "3.3.2",
    "webpack": "4.36.1",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.6",
    "yaml": "1.7.2",
    "yamljs": "0.3.0"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "parser": "typescript"
  }
}
