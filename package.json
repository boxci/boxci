{
  "name": "boxci",
  "version": "0.0.1-beta.0",
  "license": "MIT",
  "description": "The Box CI CLI. For documentation run 'boxci' or see https://boxci.dev/docs",
  "bin": "./bin/boxci.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/boxci/boxci.git"
  },
  "scripts": {
    "typecheck": "sh ./scripts/typecheck.sh",
    "test": "sh ./test/run-tests.sh",
    "integration-test": "sh ./test/integration-test.sh",
    "build": "npx --no-install webpack --config ./webpack.config.js --mode production",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@types/body-parser": "1.17.0",
    "@types/cookie-parser": "1.4.1",
    "@types/express": "4.17.0",
    "@types/ora": "3.2.0",
    "@types/shelljs": "0.8.5",
    "@types/uuid": "3.4.5",
    "body-parser": "1.19.0",
    "commander": "3.0.1",
    "cookie-parser": "1.4.4",
    "express": "4.17.1",
    "isomorphic-fetch": "2.2.1",
    "ora": "3.4.0",
    "prettier": "1.18.2",
    "shelljs": "0.8.3",
    "ts-loader": "6.0.4",
    "typescript": "3.5.3",
    "uuid": "3.3.2",
    "webpack": "4.36.1",
    "webpack-cli": "3.3.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "parser": "typescript"
  }
}
